		"Granny1": [
			{
				"Face": "granny/Laugh",
				"Voice": "granny/granny1_01_g1",
				"Text": "Aqui está tão frio,\nposso até ver meus peidos."
			},
			{
				"Face": "madeline/Default",
				"Voice": "madeline/granny1_02_m1",
				"Text": "Prazer em te ver, Vovó."
			},
			{
				"Face": "granny/Default",
				"Voice": "granny/granny1_03_g2",
				"Text": "Hah. Ei, bem-vinda de volta.\nJá faz um tempo."
			},
			{
				"Face": "madeline/Default",
				"Voice": "madeline/granny1_04_m2",
				"Text": "É, Eu estive ocupada."
			},
			{
				"Face": "granny/Default",
				"Voice": "granny/granny1_05_g3",
				"Text": "Eu ouvi!"
			},
			{
				"Face": "granny/Default",
				"Voice": "granny/granny1_06_g4",
				"Text": "A pequenina aqui está escrevendo um LIVRO!"
			},
			{
				"Face": "madeline/Upset",
				"Voice": "madeline/granny1_07_m3",
				"Text": "Oh, isso.\nNão é grande coisa..."
			},
			{
				"Face": "granny/Default",
				"Voice": "granny/granny1_08_g5",
				"Text": "ãh hã. Por isso cê tá aqui?\nNervosa sobre o grande projeto?"
			},
			{
				"Face": "madeline/Default",
				"Voice": "madeline/granny1_09_m4",
				"Text": "Não, eu tô estranhamento calma sobre o livro."
			},
			{
				"Face": "madeline/Upset",
				"Voice": "madeline/granny1_10_m5",
				"Text": "Além disso, não posso revisitar uma memória\nquerida sem ser interrogada?"
			},
			{
				"Face": "granny/Default",
				"Voice": "granny/granny1_11_g6",
				"Text": "Certo certo, Sei que tem mais\nque isso, mas num vou me meter."
			},
			{
				"Face": "granny/Default",
				"Voice": "granny/granny1_12_g7",
				"Text": "Divirta-se relembrando."
			}
		],
		"Granny2": [
			{
				"Face": "granny/Default",
				"Voice": "granny/granny2_01_g1",
				"Text": "Vamo lá, o que tá te incomodando?"
			},
			{
				"Face": "madeline/Angry",
				"Voice": "madeline/granny2_02_m1",
				"Text": "Eu tô bem. está tudo sob controle."
			},
